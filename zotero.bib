@article{berkingAffectRegulationTraining2019,
  title = {Affect Regulation Training Reduces Symptom Severity in Depression ‚Äì {{A}} Randomized Controlled Trial},
  author = {Berking, Matthias and Eichler, Eva and Luhmann, Maike and Diedrich, Alice and Hiller, Wolfgang and Rief, Winfried},
  date = {2019-08-29},

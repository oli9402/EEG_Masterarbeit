@article{batheltMoreSumIts2022,
  title = {More than the Sum of Its Parts: {{Merging}} Network Psychometrics and Network Neuroscience with Application in Autism},
  shorttitle = {More than the Sum of Its Parts},
  author = {Bathelt, Joe and Geurts, Hilde M. and Borsboom, Denny},

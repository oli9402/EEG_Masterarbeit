  author = {Grant, Bridget F. and Goldstein, Ris√´ B. and Saha, Tulshi D. and Chou, S. Patricia and Jung, Jeesun and Zhang, Haitao and Pickering, Roger P. and Ruan, W. June and Smith, Sharon M. and Huang, Boji and Hasin, Deborah S.},

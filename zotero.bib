@article{luckStandardizedMeasurementError2021,
  title = {Standardized Measurement Error: {{A}} Universal Metric of Data Quality for Averaged Event-Related Potentials},
  shorttitle = {Standardized Measurement Error},
  author = {Luck, Steven J. and Stewart, Andrew X. and Simmons, Aaron Matthew and Rhemtulla, Mijke},

@article{venegasUnderstandingLowTreatment2021,
  title = {Understanding Low Treatment Seeking Rates for Alcohol Use Disorder: {{A}} Narrative Review of the Literature and Opportunities for Improvement},
  shorttitle = {Understanding Low Treatment Seeking Rates for Alcohol Use Disorder},
  author = {Venegas, Alexandra and Donato, Suzanna and Meredith, Lindsay R. and Ray, Lara A.},
  date = {2021-11-02},
  journaltitle = {The American Journal of Drug and Alcohol Abuse},

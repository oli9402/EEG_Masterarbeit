@article{hoekstraRobustMisinterpretationConfidence2014,
  title = {üü¢{{Robust}} Misinterpretation of Confidence Intervals},
  author = {Hoekstra, Rink and Morey, Richard D. and Rouder, Jeffrey N. and Wagenmakers, Eric-Jan},

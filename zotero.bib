  title = {Alexithymia and Problematic Alcohol Use: {{A}} Critical Update},
  shorttitle = {Alexithymia and Problematic Alcohol Use},
  author = {Cruise, K. E. and Becerra, R.},
  date = {2018-02-01},
  journaltitle = {Addictive Behaviors},

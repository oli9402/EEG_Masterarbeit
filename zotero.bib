@article{fieldsHavingYourCake2020,
  title = {üü°{{Having}} Your Cake and Eating It Too: {{Flexibility}} and Power with Mass Univariate Statistics for {{ERP}} Data},
  shorttitle = {Having Your Cake and Eating It Too},
  author = {Fields, Eric C. and Kuperberg, Gina R.},
  date = {2020},
  journaltitle = {Psychophysiology},

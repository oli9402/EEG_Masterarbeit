@article{verlegerEffectsRelevanceResponse2020,
  title = {Effects of Relevance and Response Frequency on {{P3b}} Amplitudes: {{Review}} of Findings and Comparison of Hypotheses about the Process Reflected by {{P3b}}},
  shorttitle = {Effects of Relevance and Response Frequency on {{P3b}} Amplitudes},
  author = {Verleger, Rolf},
  date = {2020-07},
  journaltitle = {Psychophysiology},
